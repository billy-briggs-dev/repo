{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/sonar",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/sonar",
        "main": "libs/sonar/index.ts",
        "tsConfig": "libs/sonar/tsconfig.lib.json",
        "assets": [
          "libs/sonar/README.md",
          {
            "input": "libs/sonar",
            "glob": "**/files/**",
            "output": "/"
          },
          {
            "input": "libs/sonar",
            "glob": "**/files/**/.gitkeep",
            "output": "/"
          },
          {
            "input": "libs/sonar",
            "glob": "**/*.json",
            "ignore": ["**/tsconfig*.json", "project.json"],
            "output": "/"
          },
          {
            "input": "libs/sonar",
            "glob": "**/*.js",
            "ignore": ["**/jest.config.js"],
            "output": "/"
          },
          {
            "input": "packages/cypress",
            "glob": "**/*.d.ts",
            "output": "/"
          },
          {
            "input": "",
            "glob": "LICENSE",
            "output": "/"
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/sonar/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/sonar"],
      "options": {
        "jestConfig": "libs/sonar/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
